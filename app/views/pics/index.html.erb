<div>
  <h1 class="font-bold text-4xl">Pics#index</h1>
  <p>Find me in app/views/pics/index.html.erb</p>
</div>

<h3>User pics</h3>
<% @user.pics.each do |image| %>
  <%= image_tag (image) %>
<% end %>

<% @pics.each do |pic| %>
  <div>
    <%= link_to pic.name, pic %>
  </div>
  
<% end %>
<p>session[:user_id]: <%= session[:user_id].inspect %></p>
<%= link_to "Back", user_path(session[:user_id]) if session[:user_id].present? %> 

